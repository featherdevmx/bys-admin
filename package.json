{
  "name": "bysadmin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env next dev",
    "build": "next build && next export",
    "start": "next start",
    "eslint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "pre-commit": "lint-staged",
    "prettier": "prettier -w \"**/*.{ts,tsx}\"",
    "fix:eslint": "eslint --fix \"**/*.{ts,tsx}\"",
    "fix:prettier": "prettier --write \"**/*.{ts,tsx}\"",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "fix-lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage --runInBand --silent",
    "build-debug": "next build --debug"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.17",
    "@mui/x-data-grid": "^5.17.14",
    "@nextui-org/react": "^1.0.0-beta.10",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "axios": "^1.2.1",
    "bootstrap": "^5.2.3",
    "cross-env": "7.0.3",
    "eslint-config-next": "13.0.7",
    "formik": "^2.2.9",
    "jsonwebtoken": "^9.0.0",
    "next": "13.2.4",
    "next-base64": "^1.1.0",
    "prop-types": "^15.7.2",
    "quill": "^1.3.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hot-toast": "^2.4.0",
    "react-quill": "^2.0.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.4"
  },
  "lint-staged": {
    "./*/**/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run eslint:fix"
    ],
    "*.{json,md}": [
      "npm run prettier:fix"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
